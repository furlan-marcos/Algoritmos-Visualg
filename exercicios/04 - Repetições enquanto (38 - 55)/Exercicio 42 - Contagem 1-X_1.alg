algoritmo "1-x_1"
var
   Valor, Contador : Inteiro
inicio
      Escreva ("Digite um número: ")
      Leia (Valor)
      Contador <- 1
      Enquanto (Contador <= Valor) faca
               Escreval (Contador)
               Contador <- contador + 1
      FimEnquanto
      Escreval ("Acabou!")
fimalgoritmo
