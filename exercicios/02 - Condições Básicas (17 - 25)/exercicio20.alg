algoritmo "exercicio20"
var
   num, num2: inteiro
inicio
      escreva("Favor, insira um número inteiro difernete de ZERO: ")
      leia(num)
      num2 <- num % 2
      Se (num2 = 0) entao
         escreva("O número é par")
      senao
           escreva("O número é impar")
      fimse
fimalgoritmo
