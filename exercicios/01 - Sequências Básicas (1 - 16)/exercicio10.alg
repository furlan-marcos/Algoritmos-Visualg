algoritmo "Par_Pintar"
var
   H, L, A, T: Real
inicio
      Escreva ("Digite a altura da parede: ")
      Leia (H)
      Escreva ("Digite a largura da parede: ")
      Leia (L)
      A <- (H * L)
      T <- (A / 2)
      Escreval ("A área a ser pintada será de", A, "m².")
      Escreval ("A quantidade de tinta necessária será de", T," litros.")
fimalgoritmo
