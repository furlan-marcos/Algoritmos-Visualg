algoritmo "Eq_Seg_Grau"
var
   A, B, C, Delta : Real
inicio
      Escreva ("Digite o valor de A: ")
      Leia (A)
      Escreva ("Digite o valor de B: ")
      Leia (B)
      Escreva ("Digite o valor de C: ")
      Leia (C)
      Delta <- (B^2 - 4 * A * C)
      Escreval ("O valor de delta é igual a", Delta, ".")
      
fimalgoritmo
