algoritmo "Exercicio56"
var
   Soma, N: Inteiro
inicio
      N <- 0
      Soma <-0
      Enquanto (N <> 1111) Faca
               Escreva ("Digite um número: ")
               Leia (N)
               Soma <- Soma + N
      FimEnquanto
      Escreval ("A soma total dos números foi de", Soma, ".")
fimalgoritmo
